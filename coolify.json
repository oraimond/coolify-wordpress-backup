{
    "name": "Coolify WordPress Backup",
    "build": {
        "build_pack": "dockerfile"
    },
    "network": {
        "ports": {
            "expose": "80"
        }
    },
    "volumes": [
      {
        "source": "/var/run/docker.sock",
        "target": "/var/run/docker.sock"
      }
    ]
}